{
  "name": "sabrinarinaldi",
  "version": "1.0.0",
  "description": "Welcome to your new Jekyll theme! In this directory, you'll find the files you need to be able to package up your theme into a gem. Put your layouts in `_layouts`, your includes in `_includes`, your sass files in `_sass` and any other assets in `assets`.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./public",
    "gulp": "gulp",
    "serve:jekyll:it": "bundle exec jekyll serve --livereload --config _config.yml,_config_it.yml",
    "serve:jekyll:en": "bundle exec jekyll serve --livereload --config _config.yml,_config_en.yml",
    "build:jekyll:it": "JEKYLL_ENV=production jekyll build --config _config.yml,_config_it.yml",
    "build:jekyll:en": "JEKYLL_ENV=production jekyll build --config _config.yml,_config_en.yml",
    "serve:it": "npm-run-all clean --parallel gulp --parallel serve:jekyll:it",
    "serve:en": "npm-run-all clean --parallel gulp --parallel serve:jekyll:en",
    "build:it": "run-s clean gulp build:jekyll:it",
    "build:en": "run-s clean gulp build:jekyll:en",
    "ftp:it": "node ftp_it",
    "ftp:en": "node ftp_en",
    "deploy:it": "run-s build:it ftp:it",
    "deploy:en": "run-s build:en ftp:en"
  },
  "keywords": [],
  "author": "Raffaella Rinaldi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/raffaellarinaldi/sabrinarinaldi.git"
  },
  "dependencies": {
    "jquery": "^3.6.0",
    "uikit": "^3.10.1"
  },
  "devDependencies": {
    "basic-ftp": "^4.6.6",
    "dotenv": "^14.3.2",
    "gulp": "^4.0.2",
    "gulp-concat": "^2.6.1",
    "gulp-uglify": "^3.0.2",
    "npm-run-all": "^4.1.5"
  }
}
